{
  "name": "@vercel/edge-config",
  "version": "0.1.0",
  "description": "Ultra-low latency data at the edge",
  "homepage": "https://vercel.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/vercel/vercel.git",
    "directory": "packages/edge-config"
  },
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "eslint-check": "eslint --max-warnings=0 .",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "prepublishOnly": "pnpm run build",
    "prettier-check": "prettier --check .",
    "release": "pnpm build && changeset publish",
    "test": "pnpm run test-nodejs && pnpm run test-edge",
    "test-edge": "jest --env @edge-runtime/jest-environment",
    "test-nodejs": "jest --env node",
    "type-check": "tsc --noEmit",
    "version-packages": "changeset version"
  },
  "lint-staged": {
    "*": [
      "prettier --ignore-unknown --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0 --fix"
    ]
  },
  "prettier": "@vercel/style-guide/prettier",
  "jest": {
    "preset": "ts-jest",
    "setupFiles": [
      "<rootDir>/jest/setup.js"
    ],
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@babel/core": "7.19.6",
    "@changesets/cli": "2.25.0",
    "@edge-runtime/jest-environment": "1.1.0-beta.31",
    "@edge-runtime/types": "2.0.0",
    "@types/jest": "29.2.0",
    "@types/node": "18.11.3",
    "@vercel/style-guide": "4.0.2",
    "eslint": "8.25.0",
    "husky": "8.0.1",
    "jest": "29.2.1",
    "jest-fetch-mock": "3.0.3",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "ts-jest": "29.0.3",
    "tsup": "6.3.0",
    "typescript": "4.8.4",
    "urlpattern-polyfill": "6.0.1"
  },
  "engines": {
    "node": ">=19.0.0"
  }
}
